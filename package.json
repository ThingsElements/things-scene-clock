{
  "name": "@things-elements/things-scene-clock",
  "version": "2.0.7",
  "description": "An element providing a starting point for your own reusable Polymer elements.",
  "main": "src/index.js",
  "things-shell": true,
  "directories": {
    "test": "test"
  },
  "scripts": {
    "serve": "watchify src/*.js -o things-scene-clock.js src/index.js & polyserve",
    "build": "browserify src/index.js -o things-scene-clock.js && uglifyjs things-scene-clock.js -m -c -o things-scene-clock-min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThingsElements/things-scene-clock.git"
  },
  "author": "",
  "license": "http://things-scene.hatiolab.com/EULA/",
  "bugs": {
    "url": "https://github.com/ThingsElements/things-scene-clock/issues"
  },
  "homepage": "https://github.com/ThingsElements/things-scene-clock#readme",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "devDependencies": {
    "@hatiolab/things-shell": "^0.2.26"
  },
  "dependencies": {
    "@hatiolab/things-scene": "^2.2.36"
  }
}
